{
    "manifest_version": 3,
    "name": "Focusbook",
    "description": "An Extension",
    "version": "1.0",
    "action": 
    {
      "default_popup": "popup.html",
      "default_icon": "focusbook.png"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["contentScript.js"],
        "run_at": "document_end"
      }
    ],
    "host_permissions": ["<all_urls>"]
  }
  